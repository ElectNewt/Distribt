{
  "AppName": "Products.Consumer",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Bus": {
    "RabbitMQ": {
      "Consumer": {
        "DomainQueue" : "product-domain-queue"
      },
      "Publisher": {
        "IntegrationExchange": "products.exchange"
      }
    }
  },
  "Discovery": {
    "Address": "http://localhost:8500"
  },
  "Database": {
    "MongoDb": {
      "DatabaseName" : "distribt"
    }
  },
  "AllowedHosts": "*"
}
